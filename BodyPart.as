package {	//imports	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.TimerEvent;	import flash.display.MovieClip;		public class BodyPart{				private var _grid:Grid;		public function get grid():Grid		{ 			return _grid; 		}		public function set grid(value:Grid):void		{			if (value !== _grid)			{				if (_grid && _grid.bodyPart && _grid.bodyPart == this)					_grid.bodyPart = null;				_grid = value;				value.enemy = null;				value.bodyPart = this;			}		};				private var _State:String = "Normal";		public var isHead:Boolean = false;				public var energy:int = 100;				public function BodyPart(g:Grid){			if (g.enemy != null)				Color = g.enemy.Color;			grid = g;			//g.State = "Enemy";		}						public function set State(s:String){			if (_State == s)				return;			switch (State){				case "Normal":				default:				_State = s;			}		}				public function get State():String{			return _State;		}				public var Color:int;			}}