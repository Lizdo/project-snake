package {	//imports	import flash.events.Event;	import flash.events.MouseEvent;	import flash.events.TimerEvent;	import flash.display.MovieClip;		public class Enemy{				private var _grid:Grid;		public function get grid():Grid		{ 			return _grid; 		}		public function set grid(value:Grid):void		{			if (value !== _grid)			{				if(_grid && _grid.enemy && _grid.enemy == this)					_grid.enemy = null;				_grid = value;				value.enemy = this;			}		};				private var _Type:String = "Regular";		public function get Type():String		{ 			return _Type; 		}		public function set Type(value:String):void		{			if (validType.indexOf(value) == -1)				return;			if (value != _Type)			{				_Type = value;			}		}				//Enemy Type:		//	Speed/Energy/Absorb/Regular				private static var validType:Array = ["Speed", "Energy", "Absorb", "Regular"];				public static function getRandomType():String{			var index:int = Math.floor(Math.random() * validType.length);			return validType[index];		}				public function getColor():int{			switch (Type){				case "Speed":					return 0xDED322;				case "Energy":					return 0x44AB36;				case "Absorb":					return 0xB88421;				default:					return 0xAB2F22;			}			return		}				//DED322,44AB36,247278,B88421,AB2F22				public function Enemy(g:Grid){			grid = g;		}			}}